<KivyRelativeLayout>:
    Widget:
        size_hint: None, None
        size: 80, 80
        pos: 40, 40

        canvas:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size

    RelativeLayout:
        size_hint: None, None
        size: 80, 80
        pos: 40, 40

        canvas:
            Color:
                rgba: 1, 0, 0, 0.5
            Rectangle:
                pos: self.pos  # incorrect
                size: self.size